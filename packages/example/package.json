{
  "name": "@panhezeng/vue-tinymce-example",
  "version": "3.1.5",
  "private": true,
  "scripts": {
    "dev": "yarn run lint && vite",
    "preview": "vite preview --port 4173",
    "build": "vite build && node scripts/build.mjs",
    "lint": "yarn run type-check && cross-env NODE_ENV=production yarn run lint:es && yarn run lint:style",
    "lint:es": "cross-env NODE_ENV=production eslint --fix \"src*/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}\"",
    "lint:style": "stylelint --fix \"src*/**/*.{html,css,js,jsx,ts,tsx,vue,less}\"",
    "type-check": "vue-tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "@panhezeng/vue-tinymce": "^3.1.5",
    "tinymce": "^6.2.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "inquirer": "^9.1.4",
    "less": "^4.1.3",
    "local-web-server": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-packagejson": "^2.3.0",
    "ssh2shell": "^2.0.6",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~4.8.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9",
    "zx": "^7.1.1"
  }
